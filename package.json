  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": {
    "name": "Parker Royal",
    "url": "https://github.com/proyal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proyal/eager-cache.git"
  },
  "license": "MIT",
