  ],
  "main": "dist/src/eager-cache.js",
  "types": "dist/src/eager-cache.d.ts",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "~25.1.2",
    "@types/node": "^13.13.0",
    "@typescript-eslint/eslint-plugin": "~2.19.2",
    "@typescript-eslint/parser": "~2.19.2",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-jest": "~23.7.0",
    "jest": "~25.1.0",
    "prettier": "~1.19.1",
    "ts-jest": "~25.2.0",
    "typescript": "~3.8.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "tslib": "~1.10.0"
